{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/**/node_modules"
    ],
    "verbose": false,
    "execMap": {
        "ts": "node --require ./node_modules/.bin/ts-node/register"
    },
    "watch": [
        "server/index.ts"
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "ts"
}